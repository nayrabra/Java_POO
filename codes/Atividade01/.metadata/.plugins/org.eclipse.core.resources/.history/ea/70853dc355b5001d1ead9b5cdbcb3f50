
public class Bandas {
	
	String nome;
	String tipo;
	int integrantes;
	double lucro;
	int shows;
	
	void mostra() {
		System.out.println("Nome(s) da(s) banda(s) é/são: " + nome);
		System.out.println("Tipos(s) musical(is): " + tipo);
		System.out.println("Quantidade de integrante(s): " + integrantes);
		System.out.println("Quanto de lucro: " + lucro);
		System.out.println("Quantidade de shows: " + shows);
	}
}
