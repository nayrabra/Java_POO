
public class Principal {

	public static void main(String[] args) {
		
		Aluno aluno1 = new Aluno("Artur", 1990, 9.2);
		Turma turma1 = new Turma (aluno1, "ADS");
		
//		aluno1.novoAluno("Marcos", 1990, 8.6);
